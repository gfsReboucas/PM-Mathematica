        RowBox[{"int", "[", 
          RowBox[{
            SuperscriptBox[
              RowBox[{
                SuperscriptBox["w", 
                  TagBox[\((1, 0, 0)\),
                    Derivative],
                  MultilineFunction->None], "[", \(x, T\_0, T\_1\), "]"}], 
              "2"], ",", \({x, 0, 1}\)}], "]"}]}], "-", 
      RowBox[{\(1\/4\), " ", "\[Alpha]", " ", "\[Epsilon]", " ", 
        SuperscriptBox[
          RowBox[{"int", "[", 
            RowBox[{
              SuperscriptBox[
                RowBox[{
                  SuperscriptBox["w", 
                    TagBox[\((1, 0, 0)\),
                      Derivative],
                    MultilineFunction->None], "[", \(x, T\_0, T\_1\), "]"}], 
                "2"], ",", \({x, 0, 1}\)}], "]"}], "2"]}], "-", 
      RowBox[{\(1\/2\), " ", 
        RowBox[{"int", "[", 
          RowBox[{
