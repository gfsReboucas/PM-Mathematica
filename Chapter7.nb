              MultilineFunction->None], "[", \(x, t\), "]"}]}], "+", 
        RowBox[{"\[Alpha]", " ", "\[Epsilon]", " ", 
          RowBox[{"(", 
            RowBox[{\(\[Integral]\_0\%1\), 
              RowBox[{
                SuperscriptBox[
                  RowBox[{
                    SuperscriptBox["w", 
                      TagBox[\((1, 0)\),
                        Derivative],
                      MultilineFunction->None], "[", \(x, t\), "]"}], "2"], 
