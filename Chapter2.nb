}], "Text",
  TextAlignment->Left,
  TextJustification->1],

Cell[CellGroupData[{

Cell[BoxData[
    \(order1Eq = 
      \((eqEps[\([1, 1]\)] /. \[Zeta]\_0 -> \[Zeta]\_1)\) == 
        \((eqEps[\([1, 1]\)] /. \[Zeta]\_0 -> \[Zeta]\_1)\) - 
          \((\(eqEps[\([2, 1]\)] /. sol0\) /. \((sol0 /. conjugateRule)\))
            \)\)], "Input",
  TextAlignment->Left,
  TextJustification->1],

Cell[BoxData[
    RowBox[{
      RowBox[{\(\(-I\)\ \[Zeta]\_1[T\_0, T\_1]\), "+", 
